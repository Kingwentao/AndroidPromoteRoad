<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.androidpromoteroad">

    <application
        android:name=".kotlinbasic.BaseApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">

        <activity android:name=".drawview.DrawViewActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".annotationprocess.AnnotationMainActivity" />
        <activity android:name=".rxjava3.RxJava3Activity" />
        <activity android:name=".generic.shop.GenericActivity" />
        <activity android:name=".okhttp.OkHttpMainActivity" />
        <activity android:name=".retrofithttp.RetrofitActivity" />
        <activity android:name=".kotlinupgrade.UpgradeActivity" />
    </application>

</manifest>